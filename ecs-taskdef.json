{
  "family": "myapp-task",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::269172689648:role/ecsTaskExecutionRole",
  "containerDefinitions": [
    {
      "name": "myapp-container",
      "image": "269172689648.dkr.ecr.ap-south-1.amazonaws.com/myapp:latest",
      "portMappings": [
        {
          "containerPort": 5000,
          "hostPort": 5000,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}

